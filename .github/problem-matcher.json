{
  "problemMatcher": [
    {
      "owner": "origami-build-tools",
      "pattern": [
        {
          "regexp": "^(\\S+?):(\\d+):(\\d+)(?:.*?)- (.*?)(\\(.*?\\))?$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    }
  ]
}
