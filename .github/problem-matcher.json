{
  "problemMatcher": [
    {
      "owner": "origami-build-tools",
      "pattern": [
        {
          "regexp": "^(\\S+?):(\\d+):(\\d+)\\s+(.*?)\\s+-\\s+(.*?)(\\(.*?\\))?$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
