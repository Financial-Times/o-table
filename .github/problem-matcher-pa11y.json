{
  "problemMatcher": [
    {
      "owner": "origami-build-tools-pa11y",
      "pattern": [
        {
          "regexp": "Accessibility errors were found in (.*)",
          "file": 1,
        },
        {
          "regexp": "(\\s+)",
          "kind": "file"
        },
        {
          "regexp": "(Error:.*)",
          "message": 1,
          "kind": "file"
        }
      ]
    }
  ]
}
