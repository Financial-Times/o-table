{
  "problemMatcher": [
    {
      "owner": "origami-build-tools-mocha",
      "pattern": [
        {
          "regexp": "^\\s+(.*?):(\\d+):(\\d+) (.*?)$",
          "file": 1,
          "line": 2,
          "column": 3
        },
        {
          "regexp": "(AssertionError.*)$",
          "message": 1
        }
      ]
    }
  ]
}
