{
  "problemMatcher": [
    {
      "owner": "origami-build-tools-mocha",
      "pattern": [
        {
          "regexp": "^AssertionError: (.*?)\n+\s+at .*\n+\s+at (.*?):(\d+):(\d+)",
          "file": 2,
          "line": 3,
          "column": 4,
          "message": 1
        }
      ]
    }
  ]
}
