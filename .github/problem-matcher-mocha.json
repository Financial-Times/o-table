{
  "problemMatcher": [
    {
      "owner": "origami-build-tools-mocha",
      "pattern": [
        {
          "regexp": "^AssertionError: (.*?)\\n+\\s+",
          "message": 1
        },
        {
          "regexp": "at (test.*?):(\\d+):(\\d+)",
          "file": 1,
          "line": 2,
          "column": 3,
          "loop": true
        }
      ]
    }
  ]
}
